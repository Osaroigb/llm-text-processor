{
  "venvPath": ".",
  "venv": ".venv",
  "pythonVersion": "3.12",
  "reportMissingImports": true,
  "reportMissingModuleSource": true,
  "include": ["app"],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    ".venv",
    "tests"
  ],
  "typeCheckingMode": "basic",
  "useLibraryCodeForTypes": true
}
